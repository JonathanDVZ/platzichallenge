{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["eslint:recommend", "plugin:react/recommend", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-use-before-define": "off",
    "@typescript-eslint/no-use-before-define": ["error"],
    "rules": {
      "no-use-before-define": "off",
      "@typescript-eslint/no-use-before-define": ["error"]
    }
  }
}
